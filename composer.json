{
  "name": "linkshare/ran-reporting-api",
  "description": "API wrapper for consuming RAN reporting APIs",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Takuto Hayashi",
      "email": "takuto.hayashi@rakuten.com"
    }
  ],
  "support": {
    "issues": "https://github.com/trafficgate/ran-reporting-api/issues",
    "source": "https://github.com/trafficgate/ran-reporting-api"
  },
  "require": {
    "php": ">=7.1.3",
    "guzzlehttp/guzzle": "~6.0",
    "illuminate/support": "~5.0",
    "league/csv": "~9.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "~2.0",
    "fzaninotto/faker": "~1.8",
    "phpunit/phpunit": "~8.0",
    "psy/psysh": "~0.9"
  },
  "autoload": {
    "psr-4": {
      "Linkshare\\Api\\RanReporting\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Linkshare\\Api\\RanReporting\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
